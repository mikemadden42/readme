# http://wiki.inisec.com/index.php/Virtualbox_management_commands

VBoxManage list vms

VBoxManage showvminfo "kali"

VBoxManage startvm "kali"

VBoxHeadless --startvm "kali"

VBoxManage controlvm "kali" poweroff

VBoxManage snapshot "kali" list

VBoxManage snapshot "kali" take stable_snap

VBoxManage snapshot "kali" delete stable_snap

VBoxManage snapshot "kali" restore stable_snap
